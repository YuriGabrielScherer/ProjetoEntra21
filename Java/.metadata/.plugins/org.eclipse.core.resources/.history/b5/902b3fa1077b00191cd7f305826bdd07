package banco;

public class Cheque{

	// metodo de juros
	public void juros(double valor) {
		System.out.println("Valor simbólico: " + valor * 0.05);

	}
	
	public void mensagem() {
		System.out.println("CLASSE Mensagem");
	}
	
	public void mensagem1() {
		System.out.println("CLASSE Mensagem");
	}
}
