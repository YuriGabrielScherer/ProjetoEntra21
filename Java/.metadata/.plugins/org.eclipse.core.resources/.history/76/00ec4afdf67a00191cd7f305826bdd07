package colaborador;

public class Pessoa {

	// Atributos
	protected String nomePessoa, emailPessoa, enderecoPessoa;
	protected int idadePessoa;

	// Metodo para exibir os dados da Pessoa
	private void exibirDadosPessoa() {

		System.out.println("Nome: \t" + nomePessoa + "\nE-mail: \t" + emailPessoa + "\nEndereço: \t" + enderecoPessoa
				+ "\nIdade: \t" + idadePessoa);

	}

}
